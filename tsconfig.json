{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "es6", "es2015"],
    "types": ["cypress", "cypress-real-events"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
